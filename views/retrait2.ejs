<%- include('partials/_horizontal-navbar'); %>
    <!-- partial -->
    <div class="container-fluid page-body-wrapper">
        <div class="main-panel">
            <div class="content-wrapper">
                <div class="row">
                    <section class="forms">
                        <div class="card py-2 mb-4">
                            <div class="cards__heading">
                                <h3>Demandes de retrait <span></span></h3>
                            </div>
                            <div class="card-body">
                                <div>
                                    <table id="table_id" class="table table-responsive table-hover display">
                                        <thead class="black white-text shadow-lg">
                                            <tr>
                                                <th>Rang</th>
                                                <th>Id</th>
                                                <th>Numero</th>
                                                <th>Etat</th>
                                                <th>Date de retrait</th>
                                                <th>Action</th>
                                            </tr>
                                        </thead>
                                        <tbody>
                                            <% paycheckDemande.forEach(function(value, index){ %>
                                                <tr>
                                                    <td>
                                                        <%= index + 1 %>
                                                    </td>
                                                    <td>
                                                        <%= value.id %>
                                                    </td>
                                                    <td>
                                                        <%= value.prefix %>
                                                            <%= value.numberClient %>
                                                    </td>
                                                    <td>
                                                        <% if ( value.isActive ) { %>
                                                            <span>Actif</span>

                                                            <% }  else { %>
                                                                <span>Bloqu√©</span>
                                                                <% } %>
                                                    </td>

                                                    <td>
                                                        <script>
                                                            formatDate("<%= value.dateRetry %>")
                                                        </script>
                                                    </td>
                                                    <td>
                                                        <a type="button" target="_blank" href='/users/view?id=<%= value.id %>' class="btn btn-outline-warning btn-sm">Voir</a>


                                                    </td>
                                                </tr>
                                                <% }); %>


                                        </tbody>
                                    </table>
                                </div>

                            </div>
                        </div>


                    </section>
                </div>
            </div>

        </div>
        <!-- main-panel ends -->
    </div>

    <%- include('partials/_footer'); %>
        <script>
            $(document).ready(function() {

                $('table').addClass('nowrap').DataTable({
                    responsive: true,
                    buttons: [
                        'copy', 'excel', 'pdf'
                    ],
                });


            });
        </script>

        </body>

        </html>