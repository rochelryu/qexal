        <script>
            $(document).ready(function() {

                $(".navbottom .bottom-tabs").click(function() {
                    const attributFor = $(this).attr("for");
                    if (attributFor === "1b") {
                        location.href = location.origin + "/users/matrix"
                    } else if (attributFor === "1c") {
                        location.href = location.origin + "/other-game"
                    } else if (attributFor === "1d") {
                        location.href = location.origin + "/exchange"
                    } else {
                        location.href = location.origin + "/users"
                    }
                })




                $("#chat-circle").click(function() {
                    $(this).removeClass("is-active");
                    $(this).find("#chat-overlay").hide();
                    $("#chat-circle").toggle('scale');
                    $(".chat-box").toggle('scale');
                    $(".chat-logs").animate({
                        scrollTop: $(".chat-logs")[0].scrollHeight
                    }, 1000);
                })

                $(".chat-box-toggle").click(function() {
                    $("#chat-circle").toggle('scale');
                    $(".chat-box").toggle('scale');
                })
            })
        </script>